
drawChip = function(motorSize, motorMarginX, motorMarginZ) {
	
    var chip = drawChipMesh().rotateZ(-90).setColorName('darkGreen')

	var motor = drawCylinder().rotateX(90).scaleSize([motorSize, 15 ,motorSize]).setColorRgb(221, 226, 228);
	motor = motor.alignTo(chip, 1, 1, -1).translate([- (motorMarginX - motor.getSize("x")/2) , -4, motorMarginZ - motor.getSize("z")/2]);
	
	var screwHole = drawCylinder().scaleSize([2, 2 ,15]);
	screwHole = screwHole.alignTo(chip, 1, 0, -1).translate([-0.9, -0.5, -2]);
	
	chip = chip.subtract(screwHole);
	chip = chip.union(motor);
	chip.properties.chipScrewHole = screwHole
	chip.properties.motor = motor;
	
	return chip;
};

drawBatterySpace = function(width, batteryLength) {

    var marginLateral = 1;
	width = width + marginLateral;
    
    var battery = drawCube().scaleSize([width, batteryLength, 50]);
    
    var border = drawHexagonalPrism().rotateZ(90)
                .scaleSize([width, width/2 /* skewed */, 50])
                .alignTo(battery, 0, -1 ,0)
                .translate([0, -width/4, 0])
                ;
    var obj = battery; //.union(border);
    return obj;
};


